    "name": "Nicolas Tallefourtane",
    "url": "http://nicolab.net"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/Nicolab/atom-package-js-generator/blob/master/LICENSE"
    }
  ],
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-helpers": ">= 1.0.0 < 2.0.0",
    "fs-plus": "2.x",
    "wrench": "~1.5.1",
    "temp": "0.5.0",
    "underscore-plus": "1.x"
  }
